[fetch]
prioritize_origin = false
mirrors = [
  "https://releases.unikraft.org/mirrors",
]

[configure]
platform = "kvm"
architecture = "x86_64"

[list]
origins = [
  "https://github.com/unikraft/unikraft.git",
  "https://github.com/unikraft/plat-*",
  "https://github.com/unikraft/app-*",
  "https://github.com/unikraft/lib-*",
]
